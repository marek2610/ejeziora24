{% extends 'admin/admin_base.html.twig' %}

{% block title %}Województwa{% endblock %}

{% block page_title %}Województwa{% endblock %}

{% block body %}
<div class="table-responsive">
	<table class="table table-striped table-sm">
		<thead>
			<tr>
				<th>#</th>
				<th>Województwo</th>
				<th>Akcje</th>
			</tr>
		</thead>
		<tbody>
			{% for r in region %}
				<tr>
					<td>{{ loop.index }}</td>
					<td>{{ r.wojewodztwo }}</td>
					<td>
						<a href="{{ path('admin.region.show', {'id': r.id}) }}">show</a>
						{# <a href="{{ path('admin.region.edit', {'id': r.id}) }}">edit</a> #}
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="10">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
</div>

<nav class="navbar navbar-expand-sm">
	<ul class="navbar-nav mr-auto">
		<li class="nav-item">
			<a class="nav-link" href="{{ url('admin_admin') }}">Panel administracyjny</a>
		</li>
	</ul>
</nav>
{% endblock %}
